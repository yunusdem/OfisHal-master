			 contract="HksGenelSvc.IGenelService" name="BasicHttpBinding_IGenelService" />
			<endpoint address="https://integration.iceteknoloji.com.tr/integration.asmx"
			 binding="basicHttpsBinding" bindingConfiguration="integrationSoap"
			 contract="IceSvc.integrationSoap" name="integrationSoap" />
