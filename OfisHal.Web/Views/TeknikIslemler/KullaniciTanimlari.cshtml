@section scripts{
    <script src="~/assets/customscripts/sharedfuncs.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/flatpickr"></script>
    <script>
        var target = "";
        $(document).on("show.bs.modal", "#modal", function (e) {
            var btn = $(e.relatedTarget);
            var container = $("#modalContent");
            var label = $("#modalLabel");

            container.empty();
            label.empty();

            if (btn.data("remote-url")) {
                target = btn.data("btarget");
                container.load(btn.data("remote-url"));
                label.text(btn.data("label"))
            } else {
                target = e.relatedTarget.target;
            }
        });

        $(document).on("hidden.bs.modal", "#modal", function (e) {
            $("#modalContent").empty();
            $("#modalLabel").empty();
            if (target == "arabtn") {
                sessionStorage.getItem('kullaniciId') > 0 ? window.location.href = `/TeknikIslemler/KullaniciTanimlari?id=${sessionStorage.getItem('kullaniciId')}` : null;
                sessionStorage.removeItem("kullaniciId");
            }
        });
        $("#numeratorBtn").click(function () {
            var params = {
                satisFatSiraNo: $("#SatisFaturasiSiraNo").val(),
                alisFatSiraNo: $("#AlisFaturasiSiraNo").val(),
                MustFatSiraNo: $("#MustahsilFaturasiSiraNo").val(),
                fisSiraNo: $("#FisSiraNo").val(),
                malKabulSiraNo: $("#MalKabulSiraNo").val(),
                kullaniciId: $("#KullaniciId").val()
            };
            $.ajax({
                url: "/TeknikIslemler/NumeratorlerGuncelle",
                type: "POST",
                data: params,
                cache: false,
                success: (res) => {
                    ShowAlert(res.success, res.message);
                    $("#numModal").modal("toggle");
                }
            });
        });
        $("#navigatorSaveBtn").click(function () {
            $("form").first().trigger("submit");
        });
        $("#hideRemoveBtn").click(function () {
            DeleteConfirm().then(p => {
                if (p.isConfirmed) {
                    var id = $('#KullaniciId').val();
                    window.location.href = `/TeknikIslemler/KullaniciSil?kullaniciId=${id}`
                }
            });
        });
        $("#recordDeleteBtn").click(function () {
            $("#hideRemoveBtn").trigger("click");
        });
        function apiRequest(reqUrl, trgt) {
            $.ajax({
                url: reqUrl,
                type: "GET",
                success: function (e) {
                    loadModal(e, trgt);
                }
            });
        }
    </script>
}