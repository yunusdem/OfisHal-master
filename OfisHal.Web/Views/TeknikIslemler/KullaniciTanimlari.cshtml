@{
    ViewBag.title = "Kullanıcı Tanımları";

    ViewBag.pagetitle = "Teknik İşlemler";
    ViewBag.ptitle = "Kullanıcı Tanımları";

    ViewBag.addUrl = "/TeknikIslemler/KullaniciTanimlari?yeni=True";
    ViewBag.saveUrl = "";
    ViewBag.searchUrl = "/Common/Kullanicilar?target=test&isNavigator=true";
    ViewBag.firstDataUrl = "/TeknikIslemler/ilkSonKayit?firstOrLast=true";
    ViewBag.lastDataUrl = "/TeknikIslemler/ilkSonKayit?firstOrLast=false";
    List<TohalNumerator> numerator = (List<TohalNumerator> )ViewData["Numerator"];
    if (Model.Item1 != null)
    {
        ViewBag.beforeDataUrl = "/TeknikIslemler/sonrakiOncekiKayit?&afterOrBefore=false&currentId=" + Model.Item1.KullaniciId;
        ViewBag.afterDataUrl = "/TeknikIslemler/sonrakiOncekiKayit?&afterOrBefore=true&currentId=" + Model.Item1.KullaniciId;
        ViewBag.disabledBeforeAfter = false;
    }
    else
    {
        ViewBag.disabledBeforeAfter = true;
    }

    List<byte> yetkiler = new List<byte>();
    for (int i = 0; i < Model.Item2.Count; i++)
    {
        var yetki = Model.Item2.ElementAt(i);
        yetkiler.Add(yetki.YetkiCinsi);
    }
}
